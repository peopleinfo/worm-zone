{
  "name": "worm-zone-api",
  "version": "1.0.0",
  "description": "Multiplayer worm zone game with real-time communication",
  "main": "server/server.js",
  "scripts": {
    "start-client": "cd client && npm run dev",
    "start": "node server/server.js",
    "dev": "concurrently \"nodemon server/server.js\" \"npm run start-client\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "multiplayer",
    "game",
    "websocket",
    "realtime"
  ],
  "author": ""
}
